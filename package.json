    "@vitest/snapshot": "~3.2.4",
    "oxfmt": "^0.15.0",
    "oxlint": "^1.30.0",
    "typescript": "^5.9.3",
    "vite": "^6.3.5",
    "vite-ssr-components": "^0.5.1",
